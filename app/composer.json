{
    "name": "ilinanton/dts",
    "description": "Development Team Statistics",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Anton Ilin"
        }
    ],
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.8",
        "phpunit/phpunit": "^10.5",
        "rarst/phpcs-cognitive-complexity": "^0.2.0",
        "robmorgan/phinx": "^0.15.5",
        "dealerdirect/phpcodesniffer-composer-installer": "^1.0",
        "phpstan/phpstan": "^2.1"
    },
    "require": {
        "php": "8.4.*",
        "symfony/dotenv": "v7.0.2",
        "guzzlehttp/guzzle": "^7.8",
        "php-di/php-di": "^7.0",
        "ext-pdo": "*",
        "ext-readline": "*"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
